{
    "name": "ts-graphql-nest-boilerplate",
    "version": "0.0.1",
    "description": "typescript graphql nestjs boilerplate",
    "author": "jun<5709psy@gmail.com>",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "prebuild": "rimraf dist",
        "develop": "nodemon",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "plop": "plop",
        "dbseed": "ts-node -r tsconfig-paths/register ./src/dbseed.ts"
    },
    "dependencies": {
        "@nestjs/common": "^7.0.0",
        "@nestjs/core": "^7.0.0",
        "@nestjs/graphql": "^7.5.5",
        "@nestjs/jwt": "^7.1.0",
        "@nestjs/passport": "^7.1.0",
        "@nestjs/platform-express": "^7.0.0",
        "@nestjs/schedule": "^0.4.0",
        "@nestjs/typeorm": "^7.1.0",
        "apollo-server-core": "^2.15.1",
        "apollo-server-express": "^2.15.1",
        "aws-sdk": "^2.937.0",
        "bcrypt": "^5.0.0",
        "body-parser": "^1.19.0",
        "class-transformer": "^0.2.3",
        "class-validator": "^0.12.2",
        "compression": "^1.7.4",
        "cron": "^1.8.2",
        "dotenv": "^8.2.0",
        "express-rate-limit": "^5.1.3",
        "graphql": "^15.3.0",
        "graphql-depth-limit": "^1.1.0",
        "graphql-tools": "^6.0.12",
        "handlebars": "^4.7.7",
        "helmet": "^3.23.3",
        "jsonwebtoken": "^8.5.1",
        "mongodb": "^3.6.9",
        "mysql": "^2.18.1",
        "nodemailer": "^6.6.2",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "pluralize": "^8.0.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^6.5.4",
        "typeorm": "^0.2.25",
        "uuid": "^8.2.0"
    },
    "devDependencies": {
        "@nestjs/cli": "^7.0.0",
        "@nestjs/schematics": "^7.0.0",
        "@nestjs/testing": "^7.0.0",
        "@types/bcrypt": "^3.0.0",
        "@types/express": "^4.17.3",
        "@types/jest": "25.2.3",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/node": "^13.9.1",
        "@types/passport-jwt": "^3.0.3",
        "@types/passport-local": "^1.0.33",
        "@types/prettier": "^2.0.2",
        "@types/supertest": "^2.0.8",
        "@types/uuid": "^8.0.0",
        "@typescript-eslint/eslint-plugin": "^3.6.0",
        "@typescript-eslint/parser": "^3.6.0",
        "eslint": "^7.1.0",
        "eslint-config-airbnb": "^18.2.0",
        "eslint-config-prettier": "^6.5.0",
        "eslint-import-resolver-typescript": "^2.0.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-prettier": "^3.1.4",
        "import-sort-style-module": "^6.0.0",
        "jest": "26.0.1",
        "prettier": "^2.0.5",
        "prettier-plugin-import-sort": "0.0.4",
        "supertest": "^4.0.2",
        "ts-jest": "26.1.0",
        "ts-loader": "^6.2.1",
        "ts-node": "^8.6.2",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^3.7.4"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    },
    "importSort": {
        ".js, .jsx, .ts, .tsx": {
            "style": "module",
            "parser": "typescript"
        }
    }
}
